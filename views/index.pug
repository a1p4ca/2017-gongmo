extends layout

append body
    .hero.is-large#main-hero
        .hero-head
            include navbar
        .hero-body.has-text-centered
            .section
                .container.has-text-centered
                    p.title.has-text-white 2017 창의 IT 공모전
                    p.subtitle.has-text-white 2017년 11월 1일 - 2017년 11월 30일
                    a.button.is-large#submit-link-btn(href='/submit') 신청하기
    section.section.has-text-centered#notice: .container.slim
        h2.subtitle.is-size-2 공지사항
        each notice in notices
            .box
                article.media
                    .media-left
                        figure.image.is-64x64
                            img(src='data:image/png;base64,')
                    .media-content
                        .content
                            p
                                strong
                                  | 관리자 
                                small
                                  | 2017/11/15
                                br
                                | Lorem ipsum dolor sit amet, consectetur adipiscing elit. Aenean efficitur sit amet massa fringilla egestas. Nullam condimentum luctus turpis.

append script
    script(src='/index.bundle.js')
